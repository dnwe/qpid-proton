<FindBugsFilter>
    <!-- Allow samples to call System.exit(int) -->
	<Match>
		<Package name="com.ibm.mqlight.api.samples">
			<Bug code="DM_EXIT"/>
		</Package>
	</Match>
	
	<!-- Allow messages passed to/from Engine component to expose a byte[] -->
	<Match>
		<Class name="com.ibm.mqlight.api.impl.engine.DeliveryRequest">
			<Bug code="EI_EXPOSE_REP2"/>
		</Class>
	</Match>
	<Match>
		<Class name="com.ibm.mqlight.api.impl.engine.SendRequest">
			<Bug code="EI_EXPOSE_REP2"/>
		</Class>
	</Match>
	
</FindBugsFilter>